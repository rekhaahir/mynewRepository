package com_5_6_19_practicals;

public class Prac29 {
	public static void main(String[] args) {  
		   
int a[]= {20,3,5,3,4};
for(int i=0;i<a.length;i++)
	{
		for(int j=0;j<(a.length-1);j++)
			{
				if(a[j]>a[j+1])
				{
						int temp=a[j+1];
						a[j+1]=a[j];
						a[j]=temp;
					
				}
			}
	}
for(int j=0;j<a.length;j++)
	{
	if(a[j]==20)
	{
		System.out.println("founded");
	}
	
	System.out.println(a[j]);
	
	}
	}
	
}
